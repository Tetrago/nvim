{
	"configurations": {
		"Launch": {
			"adapter": "vscode-cpptools",
			"configuration": {
				"request": "launch",
				"program": "${workspaceRoot}/build/{{PROJECT}}.exe",
				"args": [],
				"cwd": "${workspaceRoot}",
				"externalConsole": true,
				"MIMode": "gdb",
				"setupCommands": [
					{
						"description": "Enable pretty-printing for gdb",
						"text": "-enable-pretty-printing",
						"ignoreFailures": true
					}
				]
			}
		}
	}
}
